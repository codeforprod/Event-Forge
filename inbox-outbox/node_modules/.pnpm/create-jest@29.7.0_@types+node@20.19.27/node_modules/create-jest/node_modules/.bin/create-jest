#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.27/node_modules/create-jest/bin/node_modules:/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.27/node_modules/create-jest/node_modules:/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.27/node_modules:/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.27/node_modules/create-jest/bin/node_modules:/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.27/node_modules/create-jest/node_modules:/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/create-jest@29.7.0_@types+node@20.19.27/node_modules:/private/var/folders/3s/f088p7sn0w583k6py57jw29h0000gn/T/vibe-kanban/worktrees/4142-create-universal/Event-Forge/inbox-outbox/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../bin/create-jest.js" "$@"
fi
